# JAVA_HOME
export JAVA_HOME=/usr/lib/jvm/java-11-openjdk-amd64

# PATH
export PATH=$PATH:$JAVA_HOME
export PATH=$PATH:$INSTDIR/bin
export PATH=$PATH:$INSTDIR/energyplus
export PATH=$PATH:$INSTDIR/energyplus/PreProcess
